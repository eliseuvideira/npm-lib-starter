{
  "name": "npm-lib-starter",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:eliseuvideira/npm-lib-starter.git",
  "author": "Eliseu Videira <eliseuvideira1@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "peerDependencies": {},
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage && xdg-open coverage/lcov-report/index.html &> /dev/null",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*' --ext ts",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --max-warnings=0",
      "prettier --check"
    ]
  },
  "keywords": []
}
